digraph G {
    i0_a(i) [opcode=input];
    i0_b(i) [opcode=input];
    one [opcode=const, Value='1'];
    zero [opcode=const, Value='0'];
    buf_a(i) [opcode=add];
    buf_b(i) [opcode=add];
    i1_c(i) [opcode=input];
    i1_d(i) [opcode=input];
    i0_mul [opcode=mul];
    i1_mul [opcode=mul];
    i0_add [opcode=add];
    b_add [opcode=add];
    i2_mul [opcode=mul];
    i3_mul [opcode=mul];
    i3_add [opcode=add];
    sum [opcode=output];
    sum2 [opcode=output];
    i0_a(i) -> buf_a(i);
    zero -> buf_a(i);
    i0_b(i) -> buf_b(i);
    zero -> buf_b(i);
    buf_a(i) -> i0_mul;
    buf_b(i) -> i0_mul;
    i0_mul -> i0_add;
    i0_add -> i0_add;
    i0_add -> sum;
    i1_c(i) -> i1_mul;
    i1_d(i) -> i1_mul;
    buf_b(i) -> b_add;
    one -> b_add;
    buf_a(i) -> i2_mul;
    b_add -> i2_mul;
    i1_mul -> i3_mul;
    i2_mul -> i3_mul;
    i3_mul -> i3_add;
    i3_add -> i3_add;
    i3_add -> sum2;
}
